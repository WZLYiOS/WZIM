{
  "name": "WZIM",
  "version": "1.3.8",
  "summary": "IM组件框架。",
  "description": "我主IM依赖库",
  "homepage": "https://github.com/WZLYiOS/WZIM",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "qiuqixiang": "739140860@qq.com"
  },
  "source": {
    "git": "https://github.com/WZLYiOS/WZIM.git",
    "tag": "1.3.8"
  },
  "requires_arc": true,
  "static_framework": true,
  "swift_versions": "5.0",
  "platforms": {
    "ios": "10.0"
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS": "armv7 armv7s arm64 x86_64"
  },
  "subspecs": [
    {
      "name": "Procotol",
      "source_files": "WZIM/Classes/Procotol/*",
      "dependencies": {
        "CleanJSON": [
          "~> 1.0.0"
        ]
      }
    },
    {
      "name": "Tool",
      "source_files": "WZIM/Classes/Tool/*"
    },
    {
      "name": "ToolBbar",
      "source_files": "WZIM/Classes/ToolBbar/*",
      "resources": "WZIM/**/ToolBbar.bundle",
      "dependencies": {
        "WZIM/Tool": [

        ],
        "SnapKit": [
          "~> 5.0.1"
        ],
        "WZLame": [
          "~> 4.0.0"
        ]
      }
    },
    {
      "name": "UI",
      "source_files": "WZIM/Classes/UI/**/*",
      "dependencies": {
        "WZIM/Procotol": [

        ],
        "WZIM/ToolBbar": [

        ],
        "Kingfisher": [
          "~> 5.15.0"
        ]
      },
      "resources": "WZIM/**/Cell.bundle"
    },
    {
      "name": "TIM",
      "source_files": "WZIM/Classes/TIM/*",
      "dependencies": {
        "WZIM/UI": [

        ],
        "TXIMSDK_iOS": [
          "~> 5.0.6"
        ]
      }
    },
    {
      "name": "EM",
      "source_files": "WZIM/Classes/EM/*",
      "dependencies": {
        "WZIM/UI": [

        ],
        "HyphenateLite": [
          "~> 3.7.0"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
